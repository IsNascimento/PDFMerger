<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="PDFMerger" transaction-type="RESOURCE_LOCAL">
		<class>model.Usuario</class>
		<class>model.Perfil</class>
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/pdf_merger"/>
			<property name="javax.persistence.jdbc.user" value="pdf_m"/>
			<property name="javax.persistence.jdbc.password" value="pdf.merger"/>
			<property name="eclipselink.allow-zero-id" value="true"/>
		</properties>
	</persistence-unit>
</persistence>
